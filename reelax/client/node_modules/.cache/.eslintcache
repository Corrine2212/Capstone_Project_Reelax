)\n}\nexport default Login;","/Users/ewang/Desktop/G36/codeclan_work/week_15/Capstone_Project_Reelax/reelax/client/src/containers/LiveSearch.js",[],[],"/Users/ewang/Desktop/G36/codeclan_work/week_15/Capstone_Project_Reelax/reelax/client/src/components/ForgotPassword.js",[],[],"/Users/ewang/Desktop/G36/codeclan_work/week_15/Capstone_Project_Reelax/reelax/client/src/components/MovieCard.js",[],[],{"ruleId":"65","severity":1,"message":"66","line":1,"column":8,"nodeType":"67","messageId":"68","endLine":1,"endColumn":12},{"ruleId":"65","severity":1,"message":"69","line":1,"column":17,"nodeType":"67","messageId":"68","endLine":1,"endColumn":26},{"ruleId":"65","severity":1,"message":"70","line":10,"column":19,"nodeType":"67","messageId":"68","endLine":10,"endColumn":27},{"ruleId":"65","severity":1,"message":"71","line":12,"column":12,"nodeType":"67","messageId":"68","endLine":12,"endColumn":22},{"ruleId":"65","severity":1,"message":"72","line":102,"column":11,"nodeType":"67","messageId":"68","endLine":102,"endColumn":22},{"ruleId":"65","severity":1,"message":"73","line":106,"column":11,"nodeType":"67","messageId":"68","endLine":106,"endColumn":24},{"ruleId":"65","severity":1,"message":"74","line":7,"column":12,"nodeType":"67","messageId":"68","endLine":7,"endColumn":27},{"ruleId":"65","severity":1,"message":"75","line":12,"column":12,"nodeType":"67","messageId":"68","endLine":12,"endColumn":19},{"ruleId":"65","severity":1,"message":"76","line":12,"column":21,"nodeType":"67","messageId":"68","endLine":12,"endColumn":31},{"ruleId":"65","severity":1,"message":"77","line":43,"column":11,"nodeType":"67","messageId":"68","endLine":43,"endColumn":38},{"ruleId":"65","severity":1,"message":"78","line":52,"column":11,"nodeType":"67","messageId":"68","endLine":52,"endColumn":28},"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","'Component' is defined but never used.","'setUsers' is assigned a value but never used.","'movieTitle' is assigned a value but ne